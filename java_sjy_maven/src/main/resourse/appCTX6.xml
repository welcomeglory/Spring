<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="shape" class="edu.ex.Circle">
		<property name="radius" value="10"></property>
	</bean>
	<bean id="shape" class="edu.ex.Triangle">
		<property name="width" value="10"></property>
		<property name="height" value="10"></property>		
	</bean>
	<bean id="shape" class="edu.ex.Rectangle">
		<property name="width" value="10"></property>
		<property name="height" value="10"></property>		
	</bean>	
</beans>